@model List<TestWebApp.Core.DbEntities.User>
@{
    ViewBag.Title = "Users";
}
<h2>Users</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.Name</td>
                <td>@user.Surname</td>
            </tr>
        }
    </tbody>
</table>

<button>@Html.ActionLink("Add user", "AddUser", "Admin")</button>



